{
    "excludeFiles": [
        "node_modules/**"
    ],

    "plugins": ["./lib/index.js"],

    "requireTrailingCommaInExpandedLiterals": {
        "inArrays": true,
        "inObjects": true
    },
    "disallowTrailingCommaInCollapsedLiterals": {
        "inArrays": true,
        "inObjects": true
    }
}
